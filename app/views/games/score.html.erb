<h1>Result</h1>
<ul class="letters">
  <% @letters_arr.each do |letter| %>
  <li><%= letter %></li>
  <% end %>
</ul>
<div class="user">
  <p>Your word was: <%=params[:word]%></p>
  <p> <%= @message %> </p>
</div>
<div class="score">
  <p>Your score is <%= @score %></p>
</div>
<span><%= link_to "Play Again", new_path(session[:score]) %></span>
